<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#2EA3CE" />
            <stroke
                android:width="@dimens/button_stroke_width"
                android:color="#2EA3CE" />
            <corners
                android:radius="@dimens/button_stroke_width" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#33B5E5" />
        </shape>
    </item>
</selector>

